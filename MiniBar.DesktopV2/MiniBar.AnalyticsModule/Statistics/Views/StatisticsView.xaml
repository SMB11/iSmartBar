             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" 
             xmlns:prism="http://prismlibrary.com/"
             xmlns:dxdash="http://schemas.devexpress.com/winfx/2008/xaml/dashboard" xmlns:local="clr-namespace:MiniBar.AnalyticsModule.Statistics.Views"
