<dxn:NavBarGroup x:Class="MiniBar.ProductsModule.NavBarControls"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/navbar"
             xmlns:dxInf="clr-namespace:DXInfrastructure;assembly=DXInfrastructure"
             xmlns:consts="clr-namespace:MiniBar.ProductsModule.Constants"
             mc:Ignorable="d" 
             Header="Products">
    <dxn:NavBarGroup.Items>
        <dxn:NavBarItem dxInf:BarCommandManager.CommandName="{x:Static consts:CommandNames.OpenProductManager}"  Content="Product Manager"/>
        <dxn:NavBarItem dxInf:BarCommandManager.CommandName="{x:Static consts:CommandNames.OpenBrandManager}"  Content="Brand Manager"/>
    </dxn:NavBarGroup.Items>
</dxn:NavBarGroup>
